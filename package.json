{
  "name": "ansicolor",
  "version": "2.0.1",
  "description": "A JavaScript ANSI color/style management. ANSI parsing. ANSI to CSS. Small, clean, no dependencies.",
  "main": "./build/ansicolor.js",
  "types": "./ansicolor.d.ts",
  "scripts": {
    "lint": "eslint ansicolor.js",
    "lint-test": "eslint test.js",
    "babel": "babel ansicolor.js --source-maps inline --out-file ./build/ansicolor.js",
    "build": "npm run lint && npm run lint-test && npm run babel",
    "test": "npm run build && nyc --reporter=html --reporter=text env ANSICOLOR_MODULE='./build/ansicolor' mocha --reporter spec",
    "autotest": "env ANSICOLOR_MODULE='./ansicolor' mocha --reporter spec --watch",
    "coveralls": "nyc report --reporter=text-lcov | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xpl/ansicolor.git"
  },
  "exports": {
    "import": "./build/ansicolor.mjs",
    "require": "./build/ansicolor.js",
    "types": "./ansicolor.d.ts"
  },
  "keywords": [
    "ANSI",
    "ansi to css",
    "code",
